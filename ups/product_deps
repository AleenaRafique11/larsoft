ifdh_art        v2_00_00
gcc             v4_9_3a

cetbuildtools	v5_04_03   -   only_for_build
end_product_list
  
# e10 - with gcc 4.9.3 and -std=c++14
qualifier	lareventdisplay	larexamples	larreco		larana		larpandora	gcc	ifdh_art	 larsoft_data	notes
e10:debug	e10:debug	e10:debug	e10:debug	e10:debug	e10:debug	-nq-	s44:nu:e10:debug -nq-
e10:opt		e10:opt		e10:opt		e10:opt		e10:opt		e10:opt		-nq-	s44:nu:e10:opt	 -nq-
e10:prof	e10:prof	e10:prof	e10:prof	e10:prof	e10:prof	-nq-	s44:nu:e10:prof	 -nq-
end_qualifier_list

# table file fragment to set FW_DATA
# Also need to be able to pick up fcl file includes in the current
# directory and in ./job
table_fragment_begin
    pathPrepend(FW_DATA, /nusoft/data/flux/)
    pathPrepend(FHICL_FILE_PATH, .:./job)
table_fragment_end

